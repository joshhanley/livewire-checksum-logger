{
    "name": "joshhanley/livewire-checksum-logger",
    "description": "An checksum logger for Livewire to debug corruption issues",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Hanley",
            "email": "josh@joshhanley.com.au"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "livewire/livewire": "^2.0|dev-master",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "LivewireChecksumLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewireChecksumLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewireChecksumLogger\\LivewireChecksumLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
